{
	"branches": [
		"master",
		"development"
	],
	"plugins": [
		"@semantic-release/commit-analyzer",
		[
			"@semantic-release/npm",
			{
				"npmPublish": false
			}
		],
		[
			"@axiomastudios/semantic-release-helm",
			{
				"chartRoot": "ci/helm"
			}
		],
		[
			"@semantic-release/git",
			{
				"assets": [
					"ci/helm/Chart.yaml",
					"package.json"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]"
			}
		]
	]
}