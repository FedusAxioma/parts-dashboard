{
	"name": "parts-dashboard",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"axios": "^0.21.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-icons": "^4.1.0",
		"react-scripts": "3.4.1",
		"typescript": "^3.7.5"
	},
	"devDependencies": {
		"@types/jest": "^24.9.1",
		"@types/node": "^12.12.53",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"@types/react-test-renderer": "^16.9.2",
		"@typescript-eslint/eslint-plugin": "^2.20.0",
		"@typescript-eslint/parser": "^2.20.0",
		"eslint": "^6.8.0",
		"eslint-plugin-jest": "^23.20.0",
		"node-sass": "^4.13.1",
		"react-test-renderer": "^16.13.1",
		"ts-jest": "^26.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "jest",
		"eject": "react-scripts eject",
		"lint": "eslint \"src/**/*.{js,ts,tsx}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"testRegex": "(/__tests__/.*(\\.|/)(tests|test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleNameMapper": {
			"\\.(css|sass|scss)$": "<rootDir>/__tests__/__mocks__/styleMock.js"
		},
		"setupFiles": [
			"<rootDir>/__tests__/setup.ts"
		]
	}
}
